public class Main
{
	public static void main(String[] args) {
	    int temp;int r ;
	   int sum=0;
	int  n=351;
	  temp=n;
	   while(n>0){
	       r=n%10;
	       sum=(sum*10)+r;
	       n=n/10;
	       
	   }
	       if(temp==sum)
		System.out.println("Palindrome no. ");
	   else
	     System.out.println(" Not a Palindrome no. ");  
	   
	
	}
}
